   "source": [
    "import numpy as np\n",
    "import matplotlib.pyplot as plt\n",
    "n = 4\n",
    "x = range(4)\n",
    "y = 5 + np.random.randn(100,4)\n",
    "\n",
    "plt.boxplot(y,'D')\n",
    "plt.show()"
   ]
